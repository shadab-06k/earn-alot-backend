{
  "name": "earn-alot-backend",
  "version": "1.0.0",
  "description": "Earn Alot Telegram Mini App Backend (TypeScript, Express, MongoDB, TON Integration)",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start-server": "npx pm2 start dist/server.js",
    "stop-server": "npx pm2 stop dist/server.js",
    "start": "node dist/server.js",
    "lint": "eslint 'src/**/*.{ts,tsx}' --fix"
  },
  "keywords": [
    "typescript",
    "express",
    "mongodb",
    "ton",
    "telegram-mini-app",
    "backend"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@orbs-network/ton-access": "^2.3.3",
    "@tact-lang/compiler": "^1.6.13",
    "@ton/core": "^0.61.0",
    "@ton/crypto": "^3.3.0",
    "@ton/ton": "^15.3.1",
    "@tonconnect/sdk": "^3.2.0",
    "@tonconnect/ui-react": "^2.2.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.13.0",
